<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eoma.shoppinglist">

    <application
        android:name=".AppShoppingList"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.eoma.shoppinglist.presentation.activities.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.eoma.shoppinglist.presentation.activities.newlist.ActivityNewList"
            android:exported="true"
            android:screenOrientation="portrait"/>

        <activity
            android:name="com.eoma.shoppinglist.presentation.activities.list.ActivityList"
            android:exported="true"
            android:screenOrientation="portrait"/>

    </application>

</manifest>